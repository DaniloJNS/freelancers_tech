<%=form_with model: @project do |p| %>

  <%= p.label :title, "Título" %>
  <%= p.text_field :title %>

  <%= p.label :description, "Descrição" %>
  <%= p.text_field :description %>

  <%= p.label :deadline_submission, "Receber propostas até:" %>
  <%= p.date_field :deadline_submission %>

  <%= p.submit "Enviar"%>
<% end %>
